<div class="container_1box">

  <div class="container_header">
  </div><!-- container_header -->

  <div class="unit_content">

    <div class="section">

      <%= semantic_form_for [:admin, @product, @reservation] do |f| %>

        <h3>예약 편집</h3>

        <%= render "admin/reservations/form", :f => f , :reservation => @reservation, :product => @product %>

        <%= f.submit "수정" %>
        <%= link_to "취소", [:admin, @product, :reservations], :class => "careful" %>

      <% end %>

    </div>

  </div><!-- unit_content -->

</div><!-- container_1box -->
