<div class="container_1box">

  <div class="container_header">
  </div><!-- container_header -->

  <div class="unit_content">

    <%= render "admin/products/header" %>

    <div class="nav_history">
      <%= link_to "예약목록>", [:admin, :reservations] %>
    </div>

    <div class="section">

      <%= semantic_form_for [:admin, @reservation] do |f| %>

        <h3>예약 편집</h3>

        <%= render "admin/reservations/edit_form", :f => f , :reservation => @reservation, :product => @product %>

        <%= f.submit "수정" %>
        <%= link_to "취소", [:admin, :reservations], :class => "careful" %>

      <% end %>

    </div>

  </div><!-- unit_content -->

</div><!-- container_1box -->
