<div class="container_1box">

  <div class="container_header">
  </div><!-- container_header -->

  <div class="unit_content">

    <%= render "admin/products/header" %>

    <div class="nav_history">
      <%= link_to "예약목록>", [:admin, @product, :reservations] %>
    </div>

    <div class="section">
      <%= render "reservations/section_reservation_detail", :reservation => @reservation %>
    </div>

    <div class="section">

      <%= semantic_form_for [:admin, @product, @reservation, @order] do |f| %>
        <%= render "admin/orders/edit_form", :f  => f %>

        <%= f.submit "Update" %>
      <% end %>

    </div>

  </div><!-- unit_content -->

  <div class="unit_rail">
  </div><!-- unit_rail -->

</div><!-- container_1box -->
