<div class="container_1box">

  <div class="container_header">
  </div><!-- container_header -->

  <div class="unit_content">

    <%= render "admin/products/header" %>


    <div class="section">

      <%= semantic_form_for [:admin, @product, @coupon] do |f| %>

        <h3>구매내역수정</h3>

        <%= render "admin/coupons/edit_form", :f => f, :product => @product %>

        <%= f.buttons do %>
          <%= f.submit "수정" %>
        <% end %>

      <% end %>

    </div>

  </div><!-- unit_content -->

  <div class="unit_rail">
  </div><!-- unit_rail -->

</div>
